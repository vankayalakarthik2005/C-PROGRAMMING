#include<stdio>
main()
{
    int i,n;
    n=15;
    for(i=0;i<=7;i++)
    {
        if(i%2!=0)
        printf("%d ",i);
    }
    printf("\n");
     for(i=5;i<=11;i++)
    {
        if(i%2!=0)
        printf("%d ",i);
    }
    printf("\n");
     for(i=9;i<=15;i++)
    {
        if(i%2!=0)
        printf("%d ",i);
    }
    printf("\n");
    getch();
}
