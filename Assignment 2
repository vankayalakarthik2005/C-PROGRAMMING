#include<stdio.h>
#include<conio.h>
main()
{
    int n,i,k,even,odd;
    i=1;
    clrscr();
    printf("ENTER A NUMBER");
    scanf("%d",&n);
    printf("ENTER A NUMBER TO DIVISIBLE");
    scanf("%d",&k);
    even=0;
    odd=0;
    while(i<=n)
    {
        if(i%k==0)
        {
            printf("%d\t",i);
            if(i%2==0)
            {
                even=even+i;
                
            }
            else if(i%2!=0)
            {
                odd=odd+i;
            }
            
        }
        i++;
    }
    printf("\nsum of even numbers=%d",even);
    printf("\nsum of odd numbers=%d",odd);
    getch();
}
