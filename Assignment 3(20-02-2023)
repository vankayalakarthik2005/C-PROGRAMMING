#include<stdio.h> 
main()
{
    int n,i=1,count=0;
    printf("Enter a number\n");
    scanf("%d",&n);
    while(i<=n)
    {
        if(n%i==0)
        {
            count++;
        }
        i++;
    }
    if(count==2)
    {
        printf("Prime number");
    }
    else
    {
        printf("Not a prime number");
    }
    getch();
}
