#include<stdio.h>
main()
{
    int n,count=0,i;
    printf("Enter a number\n");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    printf("%d",count);
    printf("(");
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            printf("%d,",i);
        }
    }
    printf(")");
    getch();
}
