#include<stdio.h>
main()
{
    int arm=0,n,i,rem,temp;
    printf("ENTER A NUMBER\n");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        rem=n%10;
        arm=arm+rem*rem*rem;
        n=n/10;
    }
    if(arm==temp)
    printf("Armstrong");
    else
    printf("Not a Armstrong");
    getch();
}
